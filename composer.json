{
    "name": "koshuang/laravel-hexagonal-architecture",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.46",
        "laravel/sanctum": "*",
        "laravel/tinker": "^2.7",
        "nwidart/laravel-modules": "^9.0",
        "spatie/data-transfer-object": "^3.8"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.51",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.4",
        "nunomaduro/phpinsights": "^2.11",
        "onramplab/onr-phpcs-laravel": "^1.2",
        "pestphp/pest": "^2.5",
        "phpmetrics/phpmetrics": "^2.8",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/php-file-iterator": "^4.0",
        "phpunit/phpunit": "^10.5",
        "qossmic/deptrac-shim": "^1.0",
        "rector/rector": "^1.0",
        "spatie/laravel-ignition": "^2.4"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/*"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "pest"
        ],
        "rector": [
            "rector process"
        ],
        "rector:dry-run": [
            "rector process --dry-run"
        ],
        "insights": [
            "phpinsights"
        ],
        "insights:fix": [
            "phpinsights fix"
        ],
        "phpcs:fix-pr": [
            "tools/scripts/phpcs-fix.sh pr"
        ],
        "phpcs:fix-all": [
            "PHP_CS_FIXER_IGNORE_ENV=true tools/scripts/phpcs-fix.sh all"
        ],
        "refactor": [
            "composer rector",
            "composer insights:fix",
            "composer phpcs:fix-all"
        ],
        "refactor:dry-run": [
            "composer rector:dry-run",
            "composer insights"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "prefer-stable": true
}
